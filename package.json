{
  "name": "runq",
  "description": "A lightweight, flexible task queue for Node.js and TypeScript with configurable concurrency control",
  "homepage": "https://github.com/sroettering/runq#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sroettering/runq.git"
  },
  "bugs": {
    "url": "https://github.com/sroettering/runq/issues"
  },
  "author": "sroettering",
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:compat": "npm run test:cjs && npm run test:esm && npm run test:types",
    "test:cjs": "node test-cjs.cjs",
    "test:esm": "node test-esm.mjs",
    "test:types": "tsc --noEmit test-types.mts && echo '\nðŸŽ‰ All TypeScript type tests passed!\n'",
    "prepare": "npm run build"
  },
  "keywords": [
    "queue",
    "task-queue",
    "concurrency",
    "async",
    "typescript",
    "promise",
    "batch",
    "worker",
    "fifo",
    "lifo"
  ],
  "devDependencies": {
    "@swc/core": "1.15.2",
    "@types/node": "24.10.1",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.10"
  }
}
